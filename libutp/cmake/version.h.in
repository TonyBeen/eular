#ifndef __UTP_VERSION_H__
#define __UTP_VERSION_H__

#define UTP_VERSION_STRING  "@UTP_VERSION_STRING@"

#define UTP_VERSION_MAJOR   @UTP_VERSION_MAJOR@
#define UTP_VERSION_MINOR   @UTP_VERSION_MINOR@
#define UTP_VERSION_PATCH   @UTP_VERSION_PATCH@
#define UTP_VERSION         ((UTP_VERSION_MAJOR << 16) | (UTP_VERSION_MINOR << 8) | UTP_VERSION_PATCH)

#endif // __UTP_VERSION_H__
